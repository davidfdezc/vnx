<?xml version="1.0" encoding="UTF-8"?>

<vnx_dynamips xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="/usr/share/xml/vnx/dynamips.xsd"> 

  <global>
    <sparsemem>true</sparsemem>
    <ghostios>false</ghostios>
    <hw>
       <slot id="0">NM-4E</slot>
       <slot id="1">NM-4T</slot>
     </hw>
  </global>

  <vm name="cisco">
     <conf>conf/cisco.conf</conf>
     <hw>
       <chassis>c3640</chassis>
       <slot id="0">NM-4E</slot>
       <slot id="1">NM-4T</slot>
     </hw>
     <!--login password="xxxx"/-->
     <login user="root" password="xxxx"/>
     <enable password="xxxx"/>
  </vm>

</vnx_dynamips>