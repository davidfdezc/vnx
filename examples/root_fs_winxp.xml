<domain type='kvm'>
  <name>WinXP</name>
  <memory>524288</memory>
  <vcpu>1</vcpu>
  <os>
    <type arch="i686">hvm</type>
    <boot dev='hd'/>
    <!--boot dev='cdrom'/-->
  </os>
  <features>
     <pae/>
     <acpi/>
     <apic/>
  </features>
  <clock sync="localtime"/>
  <devices>
    <emulator>/usr/bin/kvm</emulator>
    <disk type='file' device='disk'>
      <source file='/usr/share/vnuml/filesystems/root_fs_winxp'/>
      <target dev='hda'/>
    </disk>
    <!--disk type='file' device='cdrom'>
      <source file='/almacen/iso/winxp.iso'/>
      <target dev='hdb'/>
    </disk-->
    <interface type='network'>
      <source network='default'/>
    </interface>
    <interface type='network'>
      <source network='default'/>
    </interface>
    <interface type='network'>
      <source network='default'/>
    </interface>
    <interface type='network'>
      <source network='default'/>
    </interface>
     <devices>


  </devices>
    
    <!--graphics type='sdl' display=':0.0'/!-->
    <graphics type='vnc'/>
  </devices>
</domain>
